{
  "name": "harshdeep-course",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretty": "prettier --write ./",
    "update": "ncu -u",
    "-----> Tunnel <-----": "",
    "ngrok:auth": "ngrok authtoken <auth-token-goes-here>",
    "ngrok": "ngrok http http://localhost:8080",
    "cloudflare": "cloudflared tunnel --url localhost:3000",
    "-----> Shopify <-----": "",
    "g:install": "npm i -g @shopify/app@latest @shopify/cli@latest",
    "shopify": "shopify",
    "update:config": "node --loader ts-node/esm _developer/tomlWriter.ts && cd .. && shopify app deploy",
    "update:url": "node _developer/updateDashboard.js",
    "-----> Database <-----": "",
    "pg:create": "mkdir database; pg_ctl -D database init",
    "pg:start": "pg_ctl -D database start",
    "pg:stop": "pg_ctl -D database stop",
    "-----> Prisma <-----": "",
    "prisma": "npx prisma",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "-----> Reserved Scripts <-----": "",
    "prepare": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@shopify/app-bridge-react": "^4.1.3",
    "@shopify/polaris": "^13.9.0",
    "@shopify/shopify-api": "^11.2.0",
    "cryptr": "^6.3.0",
    "next": "14.2.6",
    "next-api-middleware": "^3.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@shopify/app-bridge-types": "^0.0.14",
    "@shopify/cli-kit": "^3.65.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "ngrok": "^5.0.0-beta.2",
    "npm-check-updates": "^17.0.6",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
